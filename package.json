{
  "name": "grafana-json-datasource",
  "version": "1.3.6",
  "description": "A data source plugin for loading JSON APIs into Grafana",
  "keywords": [
    "grafana",
    "json"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "changeset": "changeset-improved",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "homepage": "https://grafana.com/plugins/marcusolsson-json-datasource",
  "bugs": "https://github.com/grafana/grafana-json-datasource/issues",
  "repository": "github:grafana/grafana-json-datasource",
  "author": {
    "name": "Grafana Labs",
    "url": "https://grafana.com"
  },
  "dependencies": {
    "@grafana/plugin-ui": "^0.3.1",
    "dayjs": "^1.10.4",
    "jsonata": "^1.8.6",
    "jsonpath-plus": "^6.0.1",
    "memory-cache": "^0.2.0",
    "moment": "2.29.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@emotion/core": "10.0.27",
    "@grafana/data": "9.4.3",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/runtime": "9.4.3",
    "@grafana/toolkit": "9.4.3",
    "@grafana/ui": "9.4.3",
    "@testing-library/jest-dom": "5.4.0",
    "@testing-library/react": "^10.0.2",
    "@types/lodash": "^4.14.168",
    "@types/memory-cache": "^0.2.1",
    "@types/node": "*",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "6.30.2",
    "emotion": "10.0.27",
    "eslint": "^8.50.0",
    "eslint-webpack-plugin": "^4.0.1",
    "react-virtualized-auto-sizer": "^1.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "5.x.x",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "minimist": "1.2.8",
    "moment": "2.29.4",
    "moment-timezone": "0.5.41",
    "@grafana/data": "9.4.3",
    "@grafana/runtime": "9.4.3",
    "@grafana/toolkit": "9.4.3",
    "@grafana/ui": "9.4.3"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "yarn@1.22.19"
}
